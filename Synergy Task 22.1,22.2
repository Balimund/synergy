# coding=windows-1251
class Transport:
    def __init__(self, name, max_speed, mileage):
        self.name = name
        self.max_speed = max_speed
        self.mileage = mileage

class Autobus(Transport):
    def __init__(self, name, max_speed, mileage):
        super().__init__(name, max_speed, mileage)
        self.capacity = 50

    def seating_capacity(self):
        return f"Вместимость одного автобуса {self.name}: {self.capacity} пассажиров"

autobus = Autobus("Renaul Logan", 180, 12)
print("Название автомобиля:", autobus.name)
print("Скорость:", autobus.max_speed)
print("Пробег:", autobus.mileage)

print(autobus.seating_capacity())
