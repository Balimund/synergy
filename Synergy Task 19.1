# coding=windows-1251
def factorial(n):
    if n == 0 or n == 1:
        return 1
    else:
        return n * factorial(n-1)

def factorial_sequence(n):
    factorial_n = factorial(n)
    sequence = []
    for i in range(factorial_n, 0, -1):
        sequence.append(i)
    return sequence

number = int(input("Введите число: "))
factorial_result = factorial(number)
sequence_result = factorial_sequence(factorial_result)

print("Результирующий список:", sequence_result)
